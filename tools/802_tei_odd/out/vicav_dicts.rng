                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:seg[@type = 'hint']">
                  <s:assert test="parent::tei:quote[parent::tei:cit[@type = 'translation']]">a hint segment must be inside a translation equivalent</s:assert>
                </s:rule>
         </pattern>
         <ref name="att.global.attributes"/>
         <ref name="att.segLike.attributes"/>
         <ref name="att.typed.attribute.subtype"/>
         <ref name="att.written.attributes"/>
         <ref name="att.notated.attributes"/>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <choice>
               <value>hint</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">a hint in a translation to narrow down the semantic scope of the particular item.</a:documentation>
               <value>constrPart</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">TODO add description ???</a:documentation>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
