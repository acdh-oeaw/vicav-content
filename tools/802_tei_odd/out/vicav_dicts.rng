   <define name="cit">
      <element name="cit">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(cited quotation) contains a quotation from some other document, together with a bibliographic reference to its source. In a dictionary it may contain an example text with at least one occurrence of the word form, used in the sense being described, or a translation of the headword, or an example. [3.3.3. Quotation 4.3.1. Grouped Texts 9.3.5.1. Examples]</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="quote"/>
               <zeroOrMore>
                  <ref name="cit"/>
               </zeroOrMore>
               <ref name="fs"/>
               <ref name="bibl"/>
               <ref name="note"/>
               <ref name="sense"/>
               <ref name="usg"/>
               <ref name="xr"/>
               <ref name="def"/>
               <ref name="ptr"/>
               <ref name="ref"/>
               <ref name="gramGrp"/>
            </choice>
         </oneOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="vicav_dicts-cit-cit-context-constraint-rule-8">
            <s:rule xmlns:xi="http://www.w3.org/2001/XInclude"
                    xmlns:svg="http://www.w3.org/2000/svg"
                    xmlns:math="http://www.w3.org/1998/Math/MathML"
                    xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:cit[@type = 'translation']">
                <s:report test="* except (tei:quote|tei:usg[@type = 'hint'])">a translation equivalent must only contain of the translated text in a single quote element</s:report>
              </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="vicav_dicts-cit-cit-context-constraint-rule-9">
            <s:rule xmlns:xi="http://www.w3.org/2001/XInclude"
                    xmlns:svg="http://www.w3.org/2000/svg"
                    xmlns:math="http://www.w3.org/1998/Math/MathML"
                    xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:entry//tei:cit[@type = 'translation']">
                <s:assert test="parent::tei:sense">a translation equivalent in a headword or MWU entry must be a direct child of a sense element</s:assert>
              </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="vicav_dicts-cit-cit-context-constraint-rule-10">
            <s:rule xmlns:xi="http://www.w3.org/2001/XInclude"
                    xmlns:svg="http://www.w3.org/2000/svg"
                    xmlns:math="http://www.w3.org/1998/Math/MathML"
                    xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:cit[@subtype = 'proverb']">
                <s:assert test="@type = 'example'">unexpected value "<s:value-of select="current()/@type"/>" in @type – a proverb must be encoded as cit type="example" subtype="proverb"</s:assert>
              </s:rule>
         </pattern>
         <ref name="att.global.attributes"/>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <choice>
               <value>example</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A dictionary example. Ideally, examples should consist in complete sentences. Examples should be concise, but can also contain several sentences. If dialogical models are involved, sentences are to be separated by a dash.</a:documentation>
               <value>translation</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A translation equivalent.</a:documentation>
               <value>literalTranslation</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">TODO add difference to translation – or would’t that be a subtype="literal"? </a:documentation>
            </choice>
         </attribute>
         <optional>
            <attribute name="subtype">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <choice>
                  <value>proverb</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Proverbs are a subtype of example.</a:documentation>
                  <value>verbatim</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Proverbs are a subtype of example.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
